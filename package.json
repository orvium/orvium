{
  "name": "orvium-ui",
  "version": "0.0.10",
  "scripts": {
    "ng": "ng",
    "start": "nx serve orvium-ui",
    "build": "nx run-many --target build,build:agent --projects orvium-ui,api",
    "build:staging": "npm run build -- --configuration=staging",
    "build:prod": "npm run build -- --configuration=production",
    "package": "nx package api",
    "test": "nx run-many --target test --projects orvium-ui,api",
    "test:ci": "nx run-many --parallel=1 --target test --projects orvium-ui,api -- --ci --coverage --forceExit --runInBand",
    "unused-css": "ngx-unused-css",
    "format": "pretty-quick --branch origin/staging --pattern 'apps/*/src/**/*.{ts,html,scss}'",
    "format:check": "pretty-quick --check --staged",
    "lint": "nx run-many --target lint --projects orvium-ui,api --quiet && npm run lint:styles && npm run format:check",
    "lint:styles": "stylelint \"**/*.scss\" --quiet",
    "lint:commits": "commitlint --from origin/staging",
    "analyze:dev": "nx build && npx source-map-explorer dist/orvium-ui/browser/**/*.js",
    "analyze": "nx build --configuration production && npx source-map-explorer dist/orvium-ui/browser/**/*.js",
    "release": "standard-version --skip.commit",
    "postinstall": "husky install",
    "i18n": "nx extract-i18n",
    "gen-lib:api": "cd libs/api-client && npm run gen-sdk"
  },
  "private": true,
